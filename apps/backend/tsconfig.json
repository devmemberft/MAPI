{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "babel__generator",
      "babel__template",
      "body-parser",
      "istanbul-lib-report",
      "qs",
      "range-parser",
      "send",
      "serve-static",
      "yargs-parser"
    ]
  }
}
